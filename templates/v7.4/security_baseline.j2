config ips sensor
    edit "Standard-Outbound"
        set block-malicious-url enable
        set scan-botnet-connections block
        config entries
            edit 1
                set location server client
                set severity high critical
            next
        end
    next
end


config application list
    edit "Standard-Outbound"
        set other-application-log enable
        config entries
            edit 1
                set category 2 6
            next
        end
    next
end



