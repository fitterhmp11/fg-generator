{# Firewall address, services, and UTM that can be applied to policies #}

config firewall address
    edit "Omni-Secure-Subnet_104"
        set subnet 104.226.189.0 255.255.255.0
    next
    edit "rfc-1918_10.x"
        set subnet 10.0.0.0 255.0.0.0
    next
    edit "rfc-1918_172.16.x"
        set subnet 172.16.0.0 255.240.0.0
    next
    edit "rfc-1918_192.168.x"
        set subnet 192.168.0.0 255.255.0.0
    next
end

config firewall addrgrp
    edit "rfc-1918_grp"
        set member "rfc-1918_192.168.x" "rfc-1918_172.16.x" "rfc-1918_10.x"
    next
end

config firewall service custom
    edit "NetBios-Service"
        set tcp-portrange 137 139
        set udp-portrange 137-139
    next
end

config firewall service group
    edit "Block-Outbound"
        set member "NetBios-Service" "SMB"
    next
end


config antivirus profile
    edit "Standard-Outbound"
        config http
            set av-scan block
            set quarantine enable
        end
        config ftp
            set av-scan block
            set quarantine enable
        end
        config imap
            set av-scan block
            set quarantine enable
        end
        config pop3
            set av-scan block
            set quarantine enable
        end
        config smtp
            set av-scan block
            set quarantine enable
        end
        config cifs
            set av-scan block
            set quarantine enable
        end
    next
end



